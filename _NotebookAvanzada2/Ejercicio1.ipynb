    "else:\n",
    "    Descuento=0\n",
    "TotalCompra= cantidad - (cantidad * Descuento)\n",
    "TotalCompra= TotalCompra + (TotalCompra * Isv)\n",
    "print(f\"El total de la compra es : {TotalCompra}\")"
   ]
